# for loop example
print("For Loop Example:")

for i in range(1, 6):
    print("Number:", i)


# while loop example
print("\nWhile Loop Example:")

count = 1
while count <= 5:
    print("Count:", count)
    count += 1


# looping through a list
students = ["Maheshwar", "Raju", "Shivesh", "Narendra"]

for name in students:
    print("Student Name:", name)

#break Statement
for i in range(1, 10):
    if i == 5:
        break
    print(i)

#continue Statement
for i in range(1, 6):
    if i == 3:
        continue
    print(i)


# nested loop
for i in range(1, 4):
    for j in range(1, 4):
        print(i, j)
